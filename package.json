{
  "name": "simple-crud-api",
  "version": "1.0.0",
  "description": "express app in nest style",
  "main": "dist/main.js",
  "scripts": {
    "remove:running:port": "lsof -ti:3000 | xargs kill -9",
    "format:admin": "prettier --write \"admin/src/**/*.ts\"",
    "lint:admin": "eslint \"admin/src/**/*.{ts,tsx}\" --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "type-check": "tsc --noEmit",
    "dev": "nodemon",
    "build": "tsc && tsc-alias",
    "start": "npm run build && node dist/main.js",
    "migrate:up": "node-pg-migrate up --migrations-dir database/migrations",
    "migrate:down": "node-pg-migrate down --migrations-dir database/migrations",
    "migrate:create": "node-pg-migrate create --migrations-dir database/migrations"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "postgresql",
    "crud",
    "express"
  ],
  "author": "Senior Developer",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.726.1",
    "@aws-sdk/lib-storage": "^3.726.1",
    "@aws-sdk/s3-request-presigner": "^3.726.1",
    "bcrypt": "^6.0.0",
    "bullmq": "^5.41.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^4.17.1",
    "express-rate-limit": "^8.0.1",
    "helmet": "^8.1.0",
    "ioredis": "^5.8.0",
    "jsonwebtoken": "^9.0.2",
    "node-pg-migrate": "^8.0.4",
    "pg": "^8.16.3",
    "prom-client": "^15.1.3",
    "reflect-metadata": "^0.2.2",
    "swagger-ui-express": "^5.0.1",
    "tsconfig-paths": "^4.2.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.3.0",
    "@types/pg": "^8.15.5",
    "@types/swagger-ui-express": "^4.1.8",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.41.0"
  }
}
